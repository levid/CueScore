function updatePlayerTwoBalls() {

	var playerTwoSmallBallsIn = this.currentMatch.CurrentGame.PlayerTwoBallsHitIn;
	var playerTwoDeadBallsIn = this.currentMatch.CurrentGame.PlayerTwoDeadBalls;

	this.playerTwoSmallBallOneButton.visible = playerTwoSmallBallsIn.exists(1);
	this.playerTwoSmallBallTwoButton.visible = playerTwoSmallBallsIn.exists(2);
	this.playerTwoSmallBallThreeButton.visible = playerTwoSmallBallsIn.exists(3);
	this.playerTwoSmallBallFourButton.visible = playerTwoSmallBallsIn.exists(4);
	this.playerTwoSmallBallFiveButton.visible = playerTwoSmallBallsIn.exists(5);
	this.playerTwoSmallBallSixButton.visible = playerTwoSmallBallsIn.exists(6);
	this.playerTwoSmallBallSevenButton.visible = playerTwoSmallBallsIn.exists(7);
	this.playerTwoSmallBallEightButton.visible = playerTwoSmallBallsIn.exists(8);
	this.playerTwoSmallBallNineButton.visible = playerTwoSmallBallsIn.exists(9);

	this.playerTwoSmallBallOneDeadBall.visible = playerTwoDeadBallsIn.exists(1);
	this.playerTwoSmallBallTwoDeadBall.visible = playerTwoDeadBallsIn.exists(2);
	this.playerTwoSmallBallThreeDeadBall.visible = playerTwoDeadBallsIn.exists(3);
	this.playerTwoSmallBallFourDeadBall.visible = playerTwoDeadBallsIn.exists(4);
	this.playerTwoSmallBallFiveDeadBall.visible = playerTwoDeadBallsIn.exists(5);
	this.playerTwoSmallBallSixDeadBall.visible = playerTwoDeadBallsIn.exists(6);
	this.playerTwoSmallBallSevenDeadBall.visible = playerTwoDeadBallsIn.exists(7);
	this.playerTwoSmallBallEightDeadBall.visible = playerTwoDeadBallsIn.exists(8);
	this.playerTwoSmallBallNineDeadBall.visible = playerTwoDeadBallsIn.exists(9);

	this.playerTwoSmallBallOneShadow.visible = playerTwoSmallBallsIn.exists(1);
	this.playerTwoSmallBallTwoShadow.visible = playerTwoSmallBallsIn.exists(2);
	this.playerTwoSmallBallThreeShadow.visible = playerTwoSmallBallsIn.exists(3);
	this.playerTwoSmallBallFourShadow.visible = playerTwoSmallBallsIn.exists(4);
	this.playerTwoSmallBallFiveShadow.visible = playerTwoSmallBallsIn.exists(5);
	this.playerTwoSmallBallSixShadow.visible = playerTwoSmallBallsIn.exists(6);
	this.playerTwoSmallBallSevenShadow.visible = playerTwoSmallBallsIn.exists(7);
	this.playerTwoSmallBallEightShadow.visible = playerTwoSmallBallsIn.exists(8);
	this.playerTwoSmallBallNineShadow.visible = playerTwoSmallBallsIn.exists(9);

	this.playerTwoSmallBallOneGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 1);
	this.playerTwoSmallBallTwoGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 2);
	this.playerTwoSmallBallThreeGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 3);
	this.playerTwoSmallBallFourGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 4);
	this.playerTwoSmallBallFiveGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 5);
	this.playerTwoSmallBallSixGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 6);
	this.playerTwoSmallBallSevenGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 7);
	this.playerTwoSmallBallEightGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 8);
	this.playerTwoSmallBallNineGlow.visible = (this.currentMatch.CurrentGame.PlayerTwoLastBall == 9);
}